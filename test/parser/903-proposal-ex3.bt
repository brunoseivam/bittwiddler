func gcd:uint64 (a:uint64, b:uint64) {
    if b == 0 {
        a;              # return keyword is not necessary
    } else {
        gcd(b, a % b);
    }
}

parse {
    a : uint64;         # Read inputs from standard input
    b : uint64;
    r : uint64 = gcd(a, b);
    emit('gcd({a}, {b}) = {r}\n');
}
